generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @id @unique @default(autoincrement())
  username  String  @unique
  email     String  @unique
  firstName String?
  lastName  String?
  password  String
}
